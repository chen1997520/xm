<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qf.dao.MessageDao">
    <insert id="mess" parameterType="Message">
        insert into message(mess,uid,gid) values(#{mess},#{uid},#{gid})
    </insert>
</mapper>